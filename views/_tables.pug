    div(class="table-second tablescroll table table--2"
      role="region"
      aria-labelledby="dispatch"
      tabindex="0")
      table#table-second 
        caption#dispatch Wysyłka
        thead 
          tr(class="dark")
            th(scope="col") Waga 
            th(scope="col") Ilosc sloikow 
            th(scope="col") Ekonomiczna 
            th(scope="col") Priorytet 
        tbody 
          tr 
            td do 2kg
            td 1
            td 30zł
            td 15 zł
          tr 
            td do 5kg
            td 2
            td 20 zł
            td 25 zł
          tr 
            td do 10kg 
            td 3-5
            td 30zł
            td 15 zł
